    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@color/bg_color"> 

    <include android:id="@+id/inc_id" 
       	    layout="@layout/header"/>
    
     <ListView
        android:id="@+id/listView1"
        android:layout_width="fill_parent"
