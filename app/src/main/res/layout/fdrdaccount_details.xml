<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_color"
    android:orientation="vertical"
    android:weightSum="12.2" >

    <include
        android:id="@+id/inc_hd"
        layout="@layout/header" />

    <mbLib.CusFntTextView
        android:id="@+id/accNo"
        android:layout_width="fill_parent"
        android:layout_height="0dip"
        android:layout_gravity="center_vertical"
        android:layout_marginBottom="5dip"
        android:layout_marginTop="5dip"
        android:layout_weight="1"
        android:gravity="center_horizontal"
        android:text="@string/lbl_acc_no"
        android:textColor="@color/rep_txt_color"
        android:textStyle="bold" />

    <mbLib.CusFntTextView
        android:id="@+id/cust_name"
        android:layout_width="fill_parent"
        android:layout_height="0dip"
        android:layout_gravity="center_vertical"
        android:layout_marginBottom="5dip"
        android:layout_marginTop="5dip"
        android:layout_weight="1"
        android:gravity="center_horizontal"
        android:textColor="@color/rep_txt_color"
        android:textStyle="bold" />

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0dip"
        android:layout_marginLeft="10dip"
        android:layout_marginTop="5dip"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:weightSum="2" android:id="@+id/lyt_amt_txt" >

        <mbLib.CusFntTextView
            android:id="@+id/txtlbl__opening_amnt"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/lbl_opening_amnt"
            android:textColor="#372c24" >
        </mbLib.CusFntTextView>
        
		<mbLib.CusFntTextView
            android:id="@+id/txtlbl_maturity_amnt"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/lbl_maturity_amnt"
            android:textColor="#372c24" >
        </mbLib.CusFntTextView>
        
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0dip"
        android:layout_marginLeft="10dip"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:weightSum="2" android:id="@+id/lyt_amt_edttxt" >

        <mbLib.CustomEditText
            android:id="@+id/opening_amnt"
            android:layout_width="0dip"
            android:layout_height="fill_parent"
            android:layout_marginRight="20dip"
            android:layout_weight="1"
            android:editable="false"
            android:padding="2dip"
            android:layout_gravity="center_vertical"
            android:textColor="@color/rep_txt_color" >
        </mbLib.CustomEditText>

        <mbLib.CustomEditText
            android:id="@+id/maturity_amnt"
            android:layout_width="0dip"
            android:layout_height="fill_parent"
            android:layout_marginRight="20dip"
            android:layout_weight="1"
            android:editable="false"
            android:padding="2dip"
            android:textColor="@color/rep_txt_color" >
        </mbLib.CustomEditText>
       
    </LinearLayout>
    
    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0dip"
        android:layout_marginLeft="10dip"
        android:layout_marginTop="10dip"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:weightSum="2" >

        <mbLib.CusFntTextView
            android:id="@+id/txtlbl_opng_date"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/lbl_opng_dt"
            android:textColor="#372c24" >
        </mbLib.CusFntTextView>

        <mbLib.CusFntTextView
            android:id="@+id/txtlbl_maturity_date"
            android:layout_width="0dip"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/lbl_maturity_date"
            android:textColor="#372c24" >
        </mbLib.CusFntTextView>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0dip"
        android:layout_marginLeft="10dip"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:weightSum="2" >

         <mbLib.CustomEditText
            android:id="@+id/opng_dt"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:layout_marginRight="20dip"
            android:layout_weight="1"
            android:editable="false"
            android:padding="2dip"
            android:textColor="@color/rep_txt_color" >
        </mbLib.CustomEditText>

        <mbLib.CustomEditText
            android:id="@+id/maturity_date"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:layout_marginRight="20dip"
            android:layout_weight="1"
            android:editable="false"
            android:padding="2dip"
            android:textColor="@color/rep_txt_color" >
        </mbLib.CustomEditText>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0dip"
        android:layout_marginLeft="10dip"
        android:layout_marginTop="10dip"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:weightSum="2" 
        android:id="@+id/rdtxtextrslayout">
        
         <mbLib.CusFntTextView
            android:id="@+id/txtlbl_instl_amount"
            android:layout_width="fill_parent"
            android:layout_height="30dip"
            android:layout_weight="1"
            android:text="@string/lbl_instl_amt"
            android:textColor="#372c24" />

        <mbLib.CusFntTextView
            android:id="@+id/txtlbl_instl_frq"
            android:layout_width="fill_parent"
            android:layout_height="30dip"
            android:layout_weight="1"
            android:text="@string/lbl_instl_frq"
            android:textColor="#372c24" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0dip"
        android:layout_marginLeft="10dip"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:weightSum="2" 
        android:id="@+id/rdedtextrslayout">
        <mbLib.CustomEditText
            android:id="@+id/instmnt_amnt"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:layout_marginRight="20dip"
            android:layout_weight="1"
            android:editable="false"
            android:padding="2dip"
            android:textColor="@color/rep_txt_color" >
        </mbLib.CustomEditText>

        <mbLib.CustomEditText
            android:id="@+id/instmnt_frqncy"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:layout_marginRight="20dip"
            android:layout_weight="1"
            android:editable="false"
            android:padding="2dip"
            android:textColor="@color/rep_txt_color" >
        </mbLib.CustomEditText>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0dip"
        android:layout_marginLeft="10dip"
        android:layout_marginTop="10dip"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:weightSum="2" >

        <mbLib.CusFntTextView
            android:id="@+id/txtlbl_interest_rate"
            android:layout_width="fill_parent"
            android:layout_height="30dip"
            android:layout_weight="1"
            android:text="@string/lbl_interest_rate"
            android:textColor="#372c24" />

        <mbLib.CusFntTextView
            android:id="@+id/txtlbl_underliened"
            android:layout_width="fill_parent"
            android:layout_height="30dip"
            android:layout_weight="1"
            android:text="@string/lbl_underliened"
            android:textColor="#372c24" />
        <mbLib.CusFntTextView
            android:id="@+id/txtlbl_curbal"
            android:layout_width="fill_parent"
            android:layout_height="30dip"
            android:layout_weight="1"
            android:text="@string/current_balance"
            android:textColor="#372c24" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0dip"
        android:layout_marginLeft="10dip"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:weightSum="2" >

        <mbLib.CustomEditText
            android:id="@+id/intrest_value"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:layout_marginRight="20dip"
            android:layout_weight="1"
            android:editable="false"
            android:padding="2dip"
            android:textColor="@color/rep_txt_color" >
        </mbLib.CustomEditText>

        <mbLib.CustomEditText
            android:id="@+id/underliened"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:layout_marginRight="20dip"
            android:layout_weight="1"
            android:editable="false"
            android:padding="2dip"
            android:textColor="@color/rep_txt_color" >
        </mbLib.CustomEditText>
        
        <EditText
            android:id="@+id/curbal"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:layout_marginRight="20dip"
            android:layout_weight="1"
            android:editable="false"
            android:padding="1dip"
            android:textColor="@color/rep_txt_color" >
        </EditText>
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0dip"
        android:layout_marginLeft="10dip"
        android:layout_marginTop="10dip"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:weightSum="2" 
        android:id="@+id/intpndinsttxt">
        
         <mbLib.CusFntTextView
            android:id="@+id/txtlbl_pnd_instl"
            android:layout_width="fill_parent"
            android:layout_height="30dip"
            android:layout_weight="1"
            android:text="@string/lbl_pnd_instl"
            android:textColor="#372c24" />

        <mbLib.CusFntTextView
            android:id="@+id/txtlbl_intrvl_int"
            android:layout_width="fill_parent"
            android:layout_height="30dip"
            android:layout_weight="1"
            android:text="@string/lbl_fix_intrvl_int"
            android:textColor="#372c24" />
        <mbLib.CusFntTextView
            android:id="@+id/txtlbl_intrvl_int_frq"
            android:layout_width="fill_parent"
            android:layout_height="30dip"
            android:layout_weight="1"
            android:text="@string/lbl_intrvl_int_frq"
            android:textColor="#372c24" />
    </LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="0dip"
        android:layout_marginLeft="10dip"
        android:layout_weight="1"
        android:orientation="horizontal"
        android:weightSum="2" 
        android:id="@+id/intpndinstedt">
        <mbLib.CustomEditText
            android:id="@+id/pndt_instmnt"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:layout_marginRight="20dip"
            android:layout_weight="1"
            android:editable="false"
            android:padding="1dip"
            android:textColor="@color/rep_txt_color" >
        </mbLib.CustomEditText>

        <mbLib.CustomEditText
            android:id="@+id/intrvl_int"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:layout_marginRight="20dip"
            android:layout_weight="1"
            android:editable="false"
            android:padding="1dip"
            android:textColor="@color/rep_txt_color" >
        </mbLib.CustomEditText>
        <mbLib.CustomEditText
            android:id="@+id/intrvl_int_frq"
            android:layout_width="fill_parent"
            android:layout_height="match_parent"
            android:layout_marginRight="20dip"
            android:layout_weight="1"
            android:editable="false"
            android:padding="1dip"
            android:textColor="@color/rep_txt_color" >
        </mbLib.CustomEditText>
    </LinearLayout>

    <FrameLayout
        android:id="@+id/frame_container"
        android:layout_width="0dip"
        android:layout_height="0dip" />

</LinearLayout>